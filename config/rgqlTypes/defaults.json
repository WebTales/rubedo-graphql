{
  "Term": {
    "name": "Term",
    "multiEndpoint": "terms",
    "expose": true,
    "singleQueryArgs": {
      "id": {
        "type": "String",
        "required": true
      }
    },
    "multiQueryArgs": {
    },
    "fields": {
      "id": {
        "type": "String",
        "required": true
      },
      "text": {
        "type": "String"
      },
      "orderValue": {
        "type": "Int"
      },
      "createUser": {
        "type": "CreateUser"
      }
    },
    "connector":{
      "type":"Rubedo",
      "configs":{
        "collection":"TaxonomyTerms"
      }
    }
  },
  "Taxonomy": {
    "name": "Taxonomy",
    "singularEndpoint": "taxonomy",
    "multiEndpoint": "taxonomies",
    "expose": true,
    "singleQueryArgs": {
      "id": {
        "type": "String",
        "required": true
      }
    },
    "multiQueryArgs": {
    },
    "fields": {
      "id": {
        "type": "String",
        "required": true
      },
      "terms":{
        "type":"Term",
        "multivalued":true,
        "args":{
          "text": {
            "type": "String"
          }
        },
        "relation":{"vocabularyId":"id"}
      },
      "name": {
        "type": "String"
      },
      "multiSelect": {
        "type": "Boolean"
      },
      "readOnly": {
        "type": "Boolean"
      },
      "inputAsTree": {
        "type": "Boolean"
      },
      "createUser": {
        "type": "CreateUser"
      }
    },
    "connector":{
      "type":"Rubedo",
      "configs":{
        "collection":"Taxonomy"
      }
    }
  },
  "User": {
    "name": "user",
    "multiEndpoint": "users",
    "expose": true,
    "singleQueryArgs": {
      "id": {
        "type": "String",
        "required": true
      }
    },
    "multiQueryArgs": {
    },
    "fields": {
      "id": {
        "type": "String",
        "required": true
      },
      "name": {
        "type": "String"
      },
      "email": {
        "type": "String"
      },
      "groups":{
        "type":"Group",
        "multivalued":true,
        "relation":{"id":"groups"}
      }
    },
    "connector":{
      "type":"Rubedo",
      "configs":{
        "collection":"Users"
      }
    }
  },
  "Group": {
    "name": "Group",
    "multiEndpoint": "groups",
    "expose": true,
    "singleQueryArgs": {
      "id": {
        "type": "String",
        "required": true
      }
    },
    "multiQueryArgs": {
    },
    "fields": {
      "id": {
        "type": "String",
        "required": true
      },
      "name": {
        "type": "String"
      }
    },
    "connector":{
      "type":"Rubedo",
      "configs":{
        "collection":"Groups"
      }
    }
  },
  "Search": {
    "name": "search",
    "multiEndpoint": "searches",
    "expose": true,
    "singleQueryArgs": {
    },
    "multiQueryArgs": {
      "query": {
        "type": "String",
        "required": false
      }
    },
    "fields": {
      "id": {
        "type": "String"
      },
      "type": {
        "type":"ContentType",
        "multivalued":false,
        "relation":{"id":"typeId"}
      },
      "title": {
        "type": "String"
      },
      "score": {
        "type": "Float"
      }
    },
    "connector":{
      "type":"Rubedo",
      "configs":{
        "index":"all"
      }
    }
  },
  "ContentType": {
    "name": "contentType",
    "multiEndpoint": "contentTypes",
    "expose": true,
    "singleQueryArgs": {
      "id": {
        "type": "String",
        "required": true
      }
    },
    "multiQueryArgs": {
    },
    "fields": {
      "id": {
        "type": "String",
        "required": true
      },
      "code": {
        "type": "String"
      }
    },
    "connector":{
      "type":"Rubedo",
      "configs":{
        "collection":"ContentTypes"
      }
    }
  }
}
